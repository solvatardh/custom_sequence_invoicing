<odoo>
    <record id="seq_vendor_bill" model="ir.sequence">
        <field name="name">Vendor Bill Sequence</field>
        <field name="code">vendor.bill.sequence</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_customer_invoice" model="ir.sequence">
        <field name="name">Customer Invoice Sequence</field>
        <field name="code">customer.invoice.sequence</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_journal_entry" model="ir.sequence">
        <field name="name">Journal Entry Sequence</field>
        <field name="code">journal.entry.sequence</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
